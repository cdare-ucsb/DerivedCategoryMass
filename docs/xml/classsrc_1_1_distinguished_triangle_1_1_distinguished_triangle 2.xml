      <memberdef kind="function" id="classsrc_1_1_distinguished_triangle_1_1_distinguished_triangle_1a73195d61b76eebcc4c72e2cfa3e81249" prot="protected" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type></type>
